{
  "name": "depup-npm",
  "version": "0.0.0",
  "description": "DepUp dependency bumped npm packages",
  "scripts": {
    "cli": "node scripts/cli.mjs",
    "config": "node scripts/cli.mjs config",
    "cron:discover": "node scripts/cron-discover.mjs",
    "cron:sync": "node scripts/cron-sync.mjs",
    "add-package": "node scripts/add-package.mjs",
    "depup": "node scripts/depup.mjs",
    "depup:bump": "node scripts/depup.mjs --bump-deps",
    "depup:publish": "node scripts/depup.mjs --bump-deps --test --publish",
    "depup:test": "node scripts/depup.mjs --test",
    "example": "node scripts/example.mjs",
    "format": "prettier --write scripts/**/*.mjs",
    "integrity:report": "node scripts/integrity-meter.mjs report",
    "integrity:status": "node scripts/integrity-meter.mjs status",
    "integrity:vote": "node scripts/integrity-meter.mjs vote",
    "monitor": "node scripts/monitor.mjs",
    "monitor:check": "node scripts/monitor.mjs check",
    "monitor:report": "node scripts/monitor.mjs report",
    "heal": "node scripts/heal.mjs",
    "heal:auto": "node scripts/heal.mjs auto",
    "heal:readmes": "node scripts/heal.mjs missing-readmes",
    "heal:integrity": "node scripts/heal.mjs integrity-data",
    "interactive": "node scripts/cli.mjs interactive",
    "lint": "eslint scripts/**/*.mjs",
    "lint:fix": "eslint scripts/**/*.mjs --fix",
    "performance:test": "node scripts/performance-test.mjs",
    "readme:generate": "node scripts/generate-readme.mjs",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "security:scan": "node scripts/security-scan.mjs",
    "security:approval": "node scripts/security-approval.mjs",
    "security:approval:request": "node scripts/security-approval.mjs request",
    "security:approval:review": "node scripts/security-approval.mjs review",
    "security:approval:status": "node scripts/security-approval.mjs status",
    "security:compatibility": "node scripts/compatibility-test.mjs",
    "security:setup": "node scripts/setup-security.mjs",
    "security:setup:manual": "docker-compose build && mkdir -p security-reports",
    "security:cleanup": "docker-compose down -v && rm -rf security-reports",
    "security:demo": "node scripts/security-demo.mjs",
    "depup:secure": "docker-compose run --rm depup-sandbox",
    "depup:secure:publish": "docker-compose run --rm depup-sandbox --bump-deps --test --publish",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "inquirer": "^9.2.12",
    "npm-registry-fetch": "^16.0.0",
    "ora": "^8.0.1",
    "pacote": "^21.0.1",
    "semver": "^7.6.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "mikey-pro": "^7.2.1",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3"
  },
  "homepage": "https://github.com/depup/npm",
  "repository": "https://github.com/depup/npm",
  "bugs": "https://github.com/depup/npm/issues",
  "license": "MIT",
  "keywords": [
    "depup",
    "npm",
    "dependencies",
    "automation",
    "package-management"
  ],
  "author": "Mikl Wolfe <wolfe@mikl.io> (https://github.com/chiefmikey)",
  "prettier": "@mikey-pro/prettier-config",
  "eslintConfig": {
    "extends": "@mikey-pro/eslint-config"
  },
  "stylelint": {
    "extends": "@mikey-pro/stylelint-config"
  }
}
